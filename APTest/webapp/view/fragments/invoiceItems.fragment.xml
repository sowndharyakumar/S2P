										<suggestionItems>
											<core:ListItem text="{path:'internalOrderModel>IntOrdNumber'}" additionalText="{internalOrderModel>IntOrdDesc}"/>
										</suggestionItems>
									</Input>
									<Input value="{oInvoiceModel>profitCenter}" class="margintopinFld" tooltip="{oInvoiceModel>profitCenter}"/>
									<Input width="100%" value="{oInvoiceModel>asset}" suggestionItems="{ path:'assetModel>/d/results', templateShareable : true}"
