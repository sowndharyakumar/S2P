										<suggestionItems>
											<core:ListItem text="{path:'assetModel>Anln1'}"/>
										</suggestionItems>
									</Input>
									<Input value="{oInvoiceModel>amount}" class="margintopinFld tableItemRightAlign" change="hdrChargesAmtCalculation"
										tooltip="{oInvoiceModel>amount}"/>
									<Input width="100%" value="{oInvoiceModel>text}" class="margintopinFld" tooltip="{oInvoiceModel>text}"/>
									<Button icon="sap-icon://sys-cancel" tooltip="Delete" press="deletePlannedChargesData"
										class="tableBtnAlignMent tableSelectionChkBox glCancelBtn"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</VBox>
