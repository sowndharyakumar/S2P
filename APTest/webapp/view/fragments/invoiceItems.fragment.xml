										<suggestionItems>
											<core:ListItem text="{costCenterModel>CostCenters}" additionalText="{costCenterModel>CostCenterDesc}"/>
										</suggestionItems>
									</Input>
									<Input value="{oInvoiceModel>internalOrder}" tooltip="{oInvoiceModel>internalOrder}"
