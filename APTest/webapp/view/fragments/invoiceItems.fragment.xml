										<core:Item text="Debit" key="S"/>
										<core:Item text="Credit" key="H"/>
									</Select>
									<Input value="{oInvoiceModel>costCenter}" tooltip="{oInvoiceModel>costCenter}"
										suggestionItems="{ path:'costCenterModel>/d/results', templateShareable : true}" filterSuggests="false" showSuggestion="true"
